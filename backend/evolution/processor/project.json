{
  "name": "evolution-processor",
  "projectType": "application",
  "sourceRoot": "backend/evolution/processor/lib",
  "targets": {
    "serve": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "mix phx.server",
        "cwd": "backend/evolution/processor"
      }
    },
    "build": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "mix deps.get && mix compile",
        "cwd": "backend/evolution/processor"
      }
    },
    "test": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "mix test",
        "cwd": "backend/evolution/processor"
      }
    }
  },
  "tags": ["backend", "elixir", "events", "stream"]
}
